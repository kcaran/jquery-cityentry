{
  "name": "jquery-cityentry",
  "version": "1.0.0",
  "description": "This jQuery plugin selects a city for MA and NH. For all other states, it provides free-key entry. It relies on Chosen, a jQuery plugin that makes select boxes more user-friendly and the jQuery Validation plugin.",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-task:scss-compile": "node-sass --source-map true --include-path node_modules/normalize-scss/sass/ sass/ -o css/",
    "sass:build": "npm-run-all -p build-task:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kcaran/jquery-cityentry.git"
  },
  "author": "Keith Carangelo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kcaran/jquery-cityentry/issues"
  },
  "homepage": "https://github.com/kcaran/jquery-cityentry#readme",
  "devDependencies": {
    "node-sass": "^4.10.0",
    "normalize-scss": "7.0.1",
    "npm-run-all": "^4.1.3"
  }
}
